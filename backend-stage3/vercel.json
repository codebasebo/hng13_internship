{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/a2a/agent/codeBuddyAgent",
      "dest": "/api/a2a/agent/codeBuddyAgent.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/a2a/agent/codeBuddyAgent.js"
    }
  ]
}
